{
   "kind": "collectionType",
   "collectionName": "clients",
   "info": {
      "singularName": "client",
      "pluralName": "clients",
      "displayName": "Client"
   },
   "options": {
      "draftAndPublish": false
   },
   "pluginOptions": {},
   "attributes": {
      "name": {
         "type": "string"
      },
      "cpf": {
         "type": "string"
      },
      "phone": {
         "type": "string"
      },
      "dateOfBirth": {
         "type": "date"
      },
      "gender": {
         "type": "enumeration",
         "enum": ["Homem", "Mulher"]
      },
      "zipCode": {
         "type": "string"
      },
      "state": {
         "type": "string"
      },
      "city": {
         "type": "string"
      },
      "neighborhood": {
         "type": "string"
      },
      "address": {
         "type": "string"
      },
      "number": {
         "type": "string"
      },
      "professional_data": {
         "type": "relation",
         "relation": "oneToOne",
         "target": "api::professional-data.professional-data",
         "mappedBy": "client"
      },
      "user": {
         "type": "relation",
         "relation": "oneToOne",
         "target": "plugin::users-permissions.user",
         "inversedBy": "client"
      },
      "isActive": {
         "type": "boolean",
         "default": true
      },
      "registrationNumber": {
         "type": "string"
      },
      "requests": {
         "type": "relation",
         "relation": "oneToMany",
         "target": "api::request.request",
         "mappedBy": "client"
      }
   }
}
